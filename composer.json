{
    "name": "jimeneztdavid/package",
    "type": "standard",
    "description": "Standard for PHP package skeletons.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jimeneztdavid\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jimeneztdavid\\Package\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
